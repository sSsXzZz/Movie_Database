        <button type="button" class="btn btn-default scope_one" style="display:block;margin:auto; margin-top:10px;" id="login_request_button">Login</button>
      </div>
      <div style="float:right;">
        <button type="button" class="btn btn-default scope_one" style="display:block; margin:auto;margin-top:10px" id="signup_request_button">Sign Up</button>
      </div>
    </div>
  </div>
